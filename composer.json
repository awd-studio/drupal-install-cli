{
    "name": "awd-studio/drupal-install-cli",
    "description": "Provide a command for installing drupal with drush from the command line",
    "type": "composer-plugin",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "symfony/console": "^2.1 || ^3.0 || ^4.0",
        "drush/drush": "~8.0 || ~9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Karpov",
            "email": "awd.com.ua@gmail.com"
        }
    ],
    "bin": [
        "bin/drupal-install-cli"
    ],
    "autoload-dev": {
        "psr-4": {
            "DrupalInstallCli\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
